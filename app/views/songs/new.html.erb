<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <%= f.label :notes %>
  <input type="text" id="song_notes_attributes_0_content" name="song[note_entry][]">
  <input type="text" id="song_notes_attributes_1_content" name="song[note_entry][]">
  
  <%= f.submit %>
<% end %>

